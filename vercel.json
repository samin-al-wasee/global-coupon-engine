{
  "version": 2,
  "builds": [
    {
      "src": "globalCouponEngine/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "globalCouponEngine/wsgi.py"
    }
  ]
}
